{
  "manifest_version": 2,
  "name": "Chexxo",
  "version": "1.0",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icons/beasts-32.png",
    "default_title": "Chexxo",
    "default_popup": "popup/popup.html"
  }
}